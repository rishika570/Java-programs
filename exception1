import java.util.Scanner;
public class Array1 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int s=sc.nextInt();
        int a[]=new int[s];

        try{
            for(int i=0;i<s;i++){
                a[i]=sc.nextInt();}
            int n=sc.nextInt();
            System.out.println(a[n]);
        }
        catch(ArrayIndexOutOfBoundsException e)
        {
            System.out.println(e);

    }
        

}}
