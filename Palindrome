import java.util.*;
public class Pal {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int rev,res,rem;
        res=n;
        rev=0;
        while(res>0){
            rem=res%10;
            rev=rev*10+rem;
            res=res/10;
        }
        if(rev==n){
            System.out.println("Palindrome");
        }
        else{
            System.out.println("Not a Palindrome");
        }}}

